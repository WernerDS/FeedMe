@section Scripts {
	<script src="~/js/list.min.js"></script>
	<script>
		var options = {
		  valueNames: [ 'feed-name' ]
		};
		var userList = new List('searchFeed', options);
	</script>
	<script src="~/js/typeahead.jquery.min.js"></script>
	<script src="~/js/typeahead.config.js"></script>
	
	
	<script>
		$('#searchFeed a').click( function() {
		    $('#connectHere').removeClass('d-none')
		    $('#searchFeed').addClass('d-none')
		});
		
		$('#backToSearch').click( function() {
		    $('#connectHere').addClass('d-none')
		    $('#searchFeed').removeClass('d-none')
		});
	</script>
	
	<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
}
