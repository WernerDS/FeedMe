						<input type="text" class="form-control mb-2" id="" placeholder="username...">
						<input type="text" class="form-control" id="" placeholder="password...">
					</div>
					<a href="javascript:void(0)" class="btn btn-primary btn-big mt-2" id="connectThis">Connect</a>
