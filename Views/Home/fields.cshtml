@{
    ViewData["Title"] = "Match Fields";
}


<section class="section">
	<div class="container">
		<div class="row mb-4">
			<div class="col-md-8">
				<h2 class="display-4">@ViewData["Title"]</h2>
				<div class="badge badge-pill badge-secondary">Lists and matches</div>
				<p class="lead mt-4">Now let's match the columns in your feed to Mascus' feed.</p>
			</div>
			<div class="col-md-4 text-right">
<!-- 				<a href="/add-feed" class="btn btn-primary"><i class="fa fa-plus"></i> Create Feed</a> -->
			</div>
		</div>
		
		<table class="table table-bordered shuffle-table">
			<thead class="thead-light">
				<tr>
					<th>Mascus</th>
					<th>Toyota</th>
					<th>&nbsp;</th>
				</tr>
			</thead>
			<tbody>
				<tr>
					<td>DealerProductID<div class="badge badge-pill badge-secondary float-right">521</div></td>
					<td>ProductID</td>
					<td>&nbsp;</td>
				</tr>
				<tr>
					<td>Name<div class="badge badge-pill badge-secondary float-right">HINO 57-450 PROFIA (6X4)</div></td>
					<td>
						<ol data-draggable="target">
							<li data-draggable="item">Name</li>
						</ol>
					</td>
					<td></td>
				</tr>
				<tr>
					<td>QCategoryName<div class="badge badge-pill badge-secondary float-right">Truck Tractors</div></td>
					<td>
						<ol data-draggable="target">
							<li data-draggable="item">CategoryNumber</li>
						</ol>
					</td>
					<td></td>
				</tr>
				<tr>
					<td>InternalStockNumber<div class="badge badge-pill badge-secondary float-right">RAVT101</div></td>
					<td>
						<ol data-draggable="target">
							<li data-draggable="item">StockNumber</li>
						</ol>
					</td>
					<td></td>
				</tr>
				<tr>
					<td>Brand<div class="badge badge-pill badge-secondary float-right">HINO</div></td>
					<td>
						<ol data-draggable="target">
							<li data-draggable="item">Brand</li>
						</ol>
					</td>
					<td></td>
				</tr>
				<tr>
					<td>Model<div class="badge badge-pill badge-secondary float-right">HINO 57-450 PROFIA (6X4)</div></td>
					<td>
						<ol data-draggable="target">
							<li data-draggable="item">ItemModel</li>
						</ol>
					</td>
					<td></td>
				</tr>
				<tr>
					<td>MeterReadoutHours<div class="badge badge-pill badge-secondary float-right">285000</div></td>
					<td>
						<ol data-draggable="target">
							<li data-draggable="item">OdoHours</li>
						</ol>
					</td>
					<td></td>
				</tr>
				<tr>
					<td>Location<div class="badge badge-pill badge-secondary float-right">DURBAN</div></td>
					<td>
						<ol data-draggable="target">
							<li data-draggable="item">Location</li>
						</ol>
					</td>
					<td></td>
				</tr>
				<tr>
					<td>YearOfManufacture<div class="badge badge-pill badge-secondary float-right">2007</div></td>
					<td>
						<ol data-draggable="target">
							<li data-draggable="item">Year</li>
						</ol>
					</td>
					<td></td>
				</tr>
				<tr>
					<td>AttachmentPath1<div class="badge badge-pill badge-secondary float-right">item/5/ravt101.jpg</div></td>
					<td>
						<ol data-draggable="target">
							<li data-draggable="item">Image1</li>
						</ol>
					</td>
					<td></td>
				</tr>
			</tbody>
			<tfoot class="thead-light">
				<tr>
					<th>&nbsp;</th>
					<th>Unused</th>
					<th>&nbsp;</th>
				</tr>
				<tr>
					<td>&nbsp;</td>
					<td>
						<ol data-draggable="target">
							<li data-draggable="item">VR Photo</li>
							<li data-draggable="item">Video</li>
							<li data-draggable="item">Description</li>
						</ol>
					</td>
					<td>&nbsp;</td>
				</tr>
			</tfoot>
		</table>
		
		
		
	</div>
</section>



@section Scripts {
	<script src="~/js/shuffle.js"></script>
}
